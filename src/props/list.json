{
  "name": "AList",
  "props": {
    "bordered": {
      "value": "",
      "description": "是否展示边框",
      "default": "false",
      "type": "boolean"
    },
    "dataSource": {
      "value": "",
      "description": "列表数据源",
      "default": "-",
      "type": "any\\[]"
    },
    "footer": {
      "value": "",
      "description": "列表底部",
      "default": "-",
      "type": "string|slot"
    },
    "grid": {
      "value": "",
      "description": "列表栅格配置",
      "default": "-",
      "type": "object"
    },
    "header": {
      "value": "",
      "description": "列表头部",
      "default": "-",
      "type": "string|slot"
    },
    "itemLayout": {
      "value": "",
      "description": "设置 `List.Item` 布局, 设置成 vertical 则竖直样式显示, 默认横排",
      "default": "-",
      "type": "string"
    },
    "loading": {
      "value": "",
      "description": "当卡片内容还在加载中时，可以用 `loading` 展示一个占位",
      "default": "false",
      "type": "boolean|[object](https: |  | www.antdv.com | components | spin-cn | #api)"
    },
    "loadMore": {
      "value": "",
      "description": "加载更多",
      "default": "-",
      "type": "string|slot"
    },
    "locale": {
      "value": "",
      "description": "默认文案设置，目前包括空数据文案",
      "default": "emptyText: '暂无数据'",
      "type": "object"
    },
    "pagination": {
      "value": "",
      "description": "对应的 `pagination` [配置](https://www.antdv.com/components/pagination-cn/#api), 设置 `false` 不显示",
      "default": "false",
      "type": "boolean|object"
    },
    "renderItem": {
      "value": "",
      "description": "自定义`Item`函数，也可使用 #renderItem=\"{item, index}\"",
      "default": "",
      "type": "({item, index}) => vNode"
    },
    "rowKey": {
      "value": "",
      "description": "各项 key 的取值，可以是字符串或一个函数",
      "default": "",
      "type": "item => string|number"
    },
    "size": {
      "value": "",
      "description": "list 的尺寸",
      "default": "default",
      "type": "default | middle | small"
    },
    "split": {
      "value": "",
      "description": "是否展示分割线",
      "default": "true",
      "type": "boolean"
    }
  },
  "item": {
    "actions": {
      "value": "",
      "description": "列表操作组，根据 `itemLayout` 的不同, 位置在卡片底部或者最右侧",
      "default": "-",
      "type": "vNode[] | slot"
    },
    "extra": {
      "value": "",
      "description": "额外内容, 通常用在 `itemLayout` 为 vertical 的情况下, 展示右侧内容; `horizontal` 展示在列表元素最右侧",
      "default": "-",
      "type": "string|slot"
    }
  },
  "itemMeta": {
    "avatar": {
      "value": "",
      "description": "列表元素的图标",
      "default": "-",
      "type": "slot"
    },
    "description": {
      "value": "",
      "description": "列表元素的描述内容",
      "default": "-",
      "type": "string|slot"
    },
    "title": {
      "value": "",
      "description": "列表元素的标题",
      "default": "-",
      "type": "string|slot"
    }
  },
  "methods": [],
  "events": [],
  "listGrid": [
    { "name": "column", "description": "列数", "type": "number oneOf [ 1, 2, 3, 4, 6, 8, 12, 24]" },
    { "name": "gutter", "description": "栅格间隔", "type": "number" },
    { "name": "xxxl", "description": "`≥2000px` 展示的列数", "type": "number" },
    { "name": "xs", "description": "`<576px` 展示的列数", "type": "number" },
    { "name": "sm", "description": "`≥576px` 展示的列数", "type": "number" },
    { "name": "md", "description": "`≥768px` 展示的列数", "type": "number" },
    { "name": "lg", "description": "`≥992px` 展示的列数", "type": "number" },
    { "name": "xl", "description": "`≥1200px` 展示的列数", "type": "number" },
    { "name": "xxl", "description": "`≥1600px` 展示的列数", "type": "number" }
  ],
  "link": "https://antdv.com/components/list-cn"
}
